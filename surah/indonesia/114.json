[{"database_id":"68","surah_number":"114","verse_number":"1","ayah_text":"Katakanlah: &quot;Aku berlindung kepada Tuhan (yang memelihara dan menguasai) manusia."},{"database_id":"68","surah_number":"114","verse_number":"2","ayah_text":"Raja manusia."},{"database_id":"68","surah_number":"114","verse_number":"3","ayah_text":"Sembahan manusia."},{"database_id":"68","surah_number":"114","verse_number":"4","ayah_text":"Dari kejahatan (bisikan) syaitan yang biasa bersembunyi,"},{"database_id":"68","surah_number":"114","verse_number":"5","ayah_text":"yang membisikkan (kejahatan) ke dalam dada manusia,"},{"database_id":"68","surah_number":"114","verse_number":"6","ayah_text":"dari (golongan) jin dan manusia."}]
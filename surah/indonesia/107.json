[{"database_id":"68","surah_number":"107","verse_number":"1","ayah_text":"Tahukah kamu (orang) yang mendustakan agama?"},{"database_id":"68","surah_number":"107","verse_number":"2","ayah_text":"Itulah orang yang menghardik anak yatim,"},{"database_id":"68","surah_number":"107","verse_number":"3","ayah_text":"dan tidak menganjurkan memberi makan orang miskin."},{"database_id":"68","surah_number":"107","verse_number":"4","ayah_text":"Maka kecelakaanlah bagi orang-orang yang shalat,"},{"database_id":"68","surah_number":"107","verse_number":"5","ayah_text":"(yaitu) orang-orang yang lalai dari shalatnya,"},{"database_id":"68","surah_number":"107","verse_number":"6","ayah_text":"orang-orang yang berbuat riya,"},{"database_id":"68","surah_number":"107","verse_number":"7","ayah_text":"dan enggan (menolong dengan) barang berguna."}]